{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "quest-app",
  "version": "0.1.0",
  "identifier": "com.quest-app.app",
  "build": {
    "devUrl": "http://localhost:1420",
    "frontendDist": "../../dist/game"
  },
  "app": {
    "windows": [
      {
        "fullscreen": true,
        "resizable": true,
        "title": "Quest App",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}